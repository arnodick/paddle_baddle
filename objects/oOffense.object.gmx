          <argument>
            <kind>1</kind>
            <string>///Variables
player_num = 0;

player_stick = false;

wall[4] = 0;

colour = c_white;
tail_length = 8;
tail_width = 2;

length = 0;
length_max = 0.8

hor = 0;
ver = 0;

hp = 1;

debug = "no";

//slow_speed = 0.9;
slap_speed = 3;
decel = 40;
slap_ready = false;
slap_timer = 0;


//slap_timer = 0;
//start_slap_timer = false;
//slap_timer_threshhold = 3;
slap_length = 0.8;
if global.coolmode == true
{
    slap_length = 0;
}

last_length = 1;

coll_vert[4] = 0;
coll_horz[4] = 0;

bouncetimer = 25;
crashtimer = 70;
controltimer = 0;
control = true;

defense = 0;
sidewall = 0;
horwall = 0;
rinkside = 0;
offense = 0;

//anal_sticks[2, 4] = 0; // huhuhu, anal

anal_stick[0, 0] = gp_axislh;
anal_stick[0, 1] = gp_axislv;
anal_stick[0, 2] = gp_axisrh;
anal_stick[0, 3] = gp_axisrv;

anal_stick[1, 0] = gp_axisrh;
anal_stick[1, 1] = gp_axisrv;
anal_stick[1, 2] = gp_axislh;
anal_stick[1, 3] = gp_axislv;

key_control[0, 0] = vk_left;
key_control[0, 1] = vk_right;
key_control[0, 2] = vk_up;
key_control[0, 3] = vk_down;

key_control[1, 0] = ord('J');
key_control[1, 1] = ord('L');
key_control[1, 2] = ord('I');
key_control[1, 3] = ord('K');
</string>
