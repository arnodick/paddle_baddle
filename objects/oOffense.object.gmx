    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controls

wall[0] = floor(x);
wall[1] = floor(y);
wall[2] = ceil(x);
wall[3] = ceil(y);

defense = instance_nearest(x, y, oDefense);
sidewall = instance_nearest(x, y, oSidewall);
horwall = instance_nearest(x, y, oWall);
rinkside = instance_nearest(x, y, oSide);
offense = instance_furthest(x, y, oOffense);

if (player_num == rinkside.player_side)
{
    length_max = 0.8;
}
else
{
    length_max = 0.6;
}

if global.practice == true
{
    if instance_exists(oDefense)
    {
        if (player_num == 1)
        {    
            if instance_exists(global.defense0)
            {    
                if (irandom(6) == 0)
                {
                    if (global.defense0.y &lt; (y+(irandom(6)+2)) )
                    {
                       keyboard_key_press(ord('I'));
                       keyboard_key_release(ord('K'));
                    }
                    else if (global.defense0.y &gt; (y-(irandom(6) + 2)) )
                    {
                        keyboard_key_press(ord('K'));
                        keyboard_key_release(ord('I'));
                    }
                    else
                    {
                        keyboard_key_release(ord('I'));
                        keyboard_key_release(ord('K'));
                    }
                }
            }
            if (irandom(1) == 0)
            {
                keyboard_key_press(ord('J'));
            }
            else
            {
                keyboard_key_release(ord('J'));
            }
        }
    }
}

if (global.gamepads[player_num] == true)
{
    length = point_distance(0, 0, gamepad_axis_value(player_num, anal_stick[player_stick, 2]), gamepad_axis_value(player_num, anal_stick[player_stick, 3]));
    if (length != 0) and speed &lt; length_max and control == true //and oGame.gameover == false and oGame.goal == false and oGame.countdown == false
    {
        direction = point_direction( 0, 0, gamepad_axis_value(player_num, anal_stick[player_stick, 2]), gamepad_axis_value(player_num, anal_stick[player_stick, 3]) );
        speed = length * length_max;
        if (last_length &lt; global.dead_zone and player_num == rinkside.player_side) or global.coolmode == true //and x!= (room_width/2) - 1
        {
            if length &gt;= slap_length
            {
                speed = slap_speed;
            }
        }
    }
    else
    {
        speed -= speed/decel;
    }
    last_length = length;
    //debug = string(last_length);
}
else
{
    if keyArrowspressed(key_control) == true and speed &lt; length_max and control == true
    {
        length = length_max;
        if keyboard_check (key_control[player_num, 0]) 
            {hor = -1;}
        else if keyboard_check (key_control[player_num, 1])
            {hor = 1;}
        else
            {hor = 0;}
        if keyboard_check (key_control[player_num, 2])
            {ver = -1;}
        else if keyboard_check (key_control[player_num, 3])
            {ver = 1;}
        else
            {ver = 0;}
        direction = point_direction( 0, 0, hor, ver );
        speed = length;
        if slap_ready == true 
        {
            slap_timer += 1;
            if slap_timer &lt; 3
            {
                if player_num == rinkside.player_side
                {
                    if keyArrowspress(key_control) == true
                    {
                        speed = slap_speed;
                        slap_ready = false;
                        slap_timer = 0;
                    }
                }
                
            }
            else 
            {
                slap_ready = false;
                slap_timer = 0;
            }
        }
        else if keyArrowspress(key_control) == true
        {
            slap_ready = true;
        }
    }
    else
    {
        length = 0;
        speed -= speed/decel;
    }
    last_length = length;
}

coll_horz[0] = ceil(x + lengthdir_x(speed, direction));
coll_horz[1] = ceil(y);
coll_vert[0] = ceil(x);
coll_vert[1] = ceil(y + lengthdir_y(speed, direction));

if collHor(defense)
{
    if global.coolmode == true
    {
        hp = 0;
    }
    hspeed = -hspeed;
    controltimer = crashtimer;
    audio_play_sound(sndHurt, 1, false);
}

if collVer(defense)
{
    if global.coolmode == true
    {
        hp = 0;
    }
    if point_in_rectangle(x, y, defense.wall[0], defense.wall[1], defense.wall[2], defense.wall[3])
    {
        if defense.y &lt; y
        {
            y = defense.wall[3] + 2;
        }
        else
        {
            y = defense.wall[1] - 2;
        }
    }
    if defense.speed &gt; speed
    {
        vspeed = defense.vspeed * 1.2;
    }
    else 
    {
        vspeed =  -vspeed;
    }
    controltimer = crashtimer;
    audio_play_sound(sndHurt, 1, false);
}

if collVer(horwall)
{
    vspeed = -vspeed;
    if controltimer &lt; bouncetimer
    {
        controltimer = bouncetimer;
    }
    audio_play_sound(sndSelect, 1, false);
}

if collHor(sidewall)
{
    if player_num == sidewall.side or oGame.gameover == true
    {
        hspeed = -hspeed;
        if controltimer &lt; bouncetimer
        {
            controltimer = bouncetimer;
        }
        audio_play_sound(sndSelect, 1, false);
    }

    else
    {
        resetOffense();
        audio_play_sound(sndGoal, 1, false);
        oGame.playerpoints[player_num] += 1;
        oGame.goal = true;
        oGame.hue = colour_get_hue(oGame.playercolour[player_num]);
    }
}

if collHor(offense) or collVer(offense)
{
    if instance_number(oOffense) == 2
    {
        if offense.control == false
        {
            offense.hp = 0;
        }
    }
}
if controltimer &gt; 0
{
    control = false;
    controltimer -= 1;
}
else
{
    control = true;
}
if y &gt; room_height or y &lt; 0
{
    hp = 0;
}
if hp == 0
{
    createParticles(x, y, colour);
    audio_play_sound(sndDeath, 1, false);
    instance_destroy();
}
</string>
