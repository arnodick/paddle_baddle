            <string>///Controls
if (global.gamepads[player_num] == true)
{
    length = point_distance(0, 0, gamepad_axis_value(player_num, gp_axisrh), gamepad_axis_value(player_num, gp_axisrv));
    if (gamepad_axis_value(player_num, gp_axisrh) != 0 or gamepad_axis_value(player_num, gp_axisrv) != 0) and speed &lt; slow_speed
    {
        if last_length &lt; global.dead_zone
        {
            start_slap_timer = true;
        }
        direction = point_direction( 0, 0, gamepad_axis_value(player_num, gp_axisrh), gamepad_axis_value(player_num, gp_axisrv) );
        speed = length;
        if start_slap_timer == true and slap_timer &lt; slap_timer_threshhold
        {
            if length &gt; slap_length
            {
                speed = slap_speed;
                start_slap_timer = false;
                slap_timer = 0;
            }
            else
            {
                slap_timer += 1;
            }
        }
        else
        {
            start_slap_timer = false;
            slap_timer = 0;
        }
    }
    else
    {
        speed -= speed/decel;
    }
    last_length = length;
    debug = string(last_length);
}

paddle = instance_nearest(x, y, oDefense);

coll_vert[0] = x ;
coll_vert[1] = y + lengthdir_y(speed, direction);

if point_in_rectangle(coll_vert[0], coll_vert[1], global.bottom_wall[0], global.bottom_wall[1], global.bottom_wall[2], global.bottom_wall[3]) 
    or point_in_rectangle(coll_vert[0], coll_vert[1], global.top_wall[0], global.top_wall[1], global.top_wall[2], global.top_wall[3])
    or point_in_rectangle(coll_vert[0], coll_vert[1], paddle.defense_wall[0], paddle.defense_wall[1], paddle.defense_wall[2], paddle.defense_wall[3])
{
    vspeed = -vspeed;
}

coll_horz[0] = x + lengthdir_x(speed, direction);
coll_horz[1] = y;

if point_in_rectangle(coll_horz[0], coll_horz[1], global.bottom_wall[0], global.bottom_wall[1], global.bottom_wall[2], global.bottom_wall[3]) 
    or point_in_rectangle(coll_horz[0], coll_horz[1], global.top_wall[0], global.top_wall[1], global.top_wall[2], global.top_wall[3])
    or point_in_rectangle(coll_horz[0], coll_horz[1], paddle.defense_wall[0] - speed, paddle.defense_wall[1], paddle.defense_wall[2] +speed, paddle.defense_wall[3])
{
