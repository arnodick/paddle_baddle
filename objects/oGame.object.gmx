            <string>///Object Controller
if instance_number(oOffense) == 0
{
    createOffense(off_pos[0, 0], off_pos[0, 1], 0, global.playercolour[0]);
    createOffense(off_pos[1, 0], off_pos[1, 1], 1, global.playercolour[1]);
    resetOffense();
}
if instance_number(oOffense) == 1
{
    count += 1;
}
if count &gt;= global.deathtimer
{
    var opp = instance_nearest(x, y, oOffense);
    if opp.player_num == 0
    {
        var num = 1;
    }
    else
    {
        var num = 0;
    }
    createOffense(off_pos[num, 0], off_pos[num, 1], num, global.playercolour[num]);
    createParticles(off_pos[num, 0], off_pos[num, 1], global.playercolour[num]);
    audio_play_sound(sndRespawn, 1, false);
    count = 0;
}
if global.countdown == true
{
    countdowntimer += 1;
    if countdowntimer &gt;= global.crashtimer
    {
        global.countdown = false;
        countdowntimer = 0;
    }
}

if global.playerpoints[0] &gt;= 5 or global.playerpoints[1] &gt;= 5
{
    global.gameover = true;
    var i;
    for (i = 0; i &lt; 2; i++)
    {
        if global.playerpoints[i] &gt;= 5
        {
            global.winner = i + 1;
        }
    }
}

