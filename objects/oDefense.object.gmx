        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controls

colour = oGame.base_colour;

top = y;
bottom = y + length;

wall[0] = x - 1; // this causes a bug: if pixel is directly next to left paddle, will go through with slap shot (width of paddle wall = 0)
wall[1] = top;
wall[2] = x;
wall[3] = bottom;

if (global.gamepads[player_num] == true)
{
    if gamepad_axis_value(player_num, anal_stick[player_num, 1]) &gt; 0 and bottom + speed &lt; room_height and oGame.gameover == false
    {
        vspeed = move_speed;   
    }
    else if (gamepad_axis_value(player_num, anal_stick[player_num, 1]) &lt; 0) and (top + speed &gt; 0) and oGame.gameover == false
    {
        vspeed = -move_speed;   
    }
    /*
    else if ( vspeed != 0 ) and (bottom + vspeed &lt; room_height) and (top + vspeed &gt; 0)
    {
        vspeed += -vspeed/8;
    }
    */
    else
    {
        vspeed = 0;
    }
}
else
{
    if keyboard_check(key_control[player_num, 2]) and top + speed &gt; 0 and oGame.gameover == false
    {
        vspeed = -move_speed;   
    }
    else if keyboard_check(key_control[player_num, 3]) and bottom + speed &lt; room_height  and oGame.gameover == false
    {
        vspeed = move_speed;   
    }
    else
    {
        vspeed = 0;
    }
}
</string>
